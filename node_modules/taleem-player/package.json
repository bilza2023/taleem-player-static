{
  "name": "taleem-player",
  "version": "1.0.14",
  "type": "module",
  "main": "./dist/taleem-player.umd.js",
  "module": "./dist/taleem-player.esm.js",
  "exports": {
    ".": {
      "import": "./dist/taleem-player.esm.js",
      "require": "./dist/taleem-player.umd.js"
    },
    "./math": {
      "import": "./dist/math/useMath.js"
    },
    "./validation": {
      "import": "./dist/validation/index.js"
    },
    "./spec": {
      "import": "./dist/spec/index.js"
    },
    "./css": "./dist/css/taleem.css",
    "./css/dark": "./dist/css/themes/dark.css",
    "./css/light": "./dist/css/themes/light.css",
    "./css/paper": "./dist/css/themes/paper.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node ./scripts/build.js",
    "dev": "vite",
    "test": "vitest run"
  },
  "dependencies": {
    "katex": "^0.16.28"
  },
  "devDependencies": {
    "esbuild": "^0.27.2",
    "jsdom": "^22.1.0",
    "vite": "^7.3.1",
    "vitest": "^3.2.4",
    "zod": "^4.3.6"
  }
}
